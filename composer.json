{
  "name": "nystudio107/bundle-installer",
  "description": "Install, update, and manage Twig template bundles via Composer",
  "version": "1.0.0",
  "keywords": [
    "twig",
    "composer",
    "installer",
    "bundle",
    "craftcms",
    "drupal",
    "symphony"
  ],
  "homepage": "https://nystudio107.com",
  "type": "composer-plugin",
  "support": {
    "email": "info@nystudio107.com",
    "issues": "https://github.com/nystudio107/bundle-installer/issues",
    "source": "https://github.com/nystudio107/bundle-installer",
    "docs": "https://github.com/nystudio107/bundle-installer"
  },
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "composer-plugin-api": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "nystudio107\\composer\\": "src/"
    }
  },
  "extra": {
    "class": "nystudio107\\composer\\Plugin"
  }
}
